{
  "name": "http-dsl",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "format": "prettier --write src/",
    "test": "vitest",
    "test:run": "vitest run",
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "npm run format && vitest run && tsc",
    "start": "tsc && node dist/index.js examples/example.txt",
    "symmetric": "tsc && node dist/index.js temp/symmetric.txt",
    "repl": "tsc && node dist/index.js",
    "scm": "tsc && node dist/index.js examples/scm.txt"
  },
  "author": "John Egan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
